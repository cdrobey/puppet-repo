defaultEntryPoints = ["http"]

[entryPoints]
  [entryPoints.http]
    address = ":80"
  [entryPoints.api]
    address = ":10000"

[api]
  entryPoint = "api"
  dashboard = true

[docker]
  endpoint = "unix:///var/run/docker.sock"
  domain = "local.familyroberson.com"
  exposedByDefault = true

[log]
  filePath = "/var/log/traefik.log"
