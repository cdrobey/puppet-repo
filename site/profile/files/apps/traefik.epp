defaultEntryPoints = ["http", "https"]

[entryPoints]
  [entryPoints.http]
  address = ":80"
  [entryPoints.https]
  address = ":443"
    [entryPoints.https.tls]


[docker]
endpoint = "unix:///var/run/docker.sock"
domain = "familyroberson.com"
exposedByDefault = false

[api]
  entryPoint = "traefik"
  dashboard = true
